{
    "name": "ruzaik11/laravel-seeder-tracker",
    "description": "Track Laravel seeders execution like migrations with batch support and status management",
    "keywords": [
        "laravel",
        "seeder", 
        "database",
        "tracking",
        "migration",
        "batch",
        "performance",
        "artisan"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ruzaik Nazeer",
            "email": "ruzaiknazeer@gmail.com",
            "homepage": "https://ruzaik.dev",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/ruzaiknazeer/laravel-seeder-tracker",
    "support": {
        "issues": "https://github.com/ruzaiknazeer/laravel-seeder-tracker/issues",
        "source": "https://github.com/ruzaiknazeer/laravel-seeder-tracker"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Ruzaik\\SeederTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ruzaik\\SeederTracker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ruzaik\\SeederTracker\\SeederTrackerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
